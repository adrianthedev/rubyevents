<% if @found_account.present? %>
  <%= render partial: "friend_prompt" %>
<% else %>
  <% if Current.user.present? %>
    <% if Current.user.passport_account.present? %>
      <%= render partial: "no_profile_found" %>
    <% else %>
      <%= render partial: "claim_profile" %>
    <% end %>
  <% else %>
    <%= render partial: "claim_profile" %>
  <% end %>
<% end %>
